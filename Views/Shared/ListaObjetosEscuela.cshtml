@* @model List<ObjetoEscuelaBase> *@

<table class="text-center table table-dark table-hover table-sm">
    <thead>
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">ID</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var alumno in Model){
            <tr>
                <td>@alumno.Nombre</td>
                <td>@alumno.UniqueId</td>
            </tr>
        }
    </tbody>
</table>